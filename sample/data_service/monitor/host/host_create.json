{

  "method": "host.create",
    "params": {
        "host": "{{host}}",
        "name": "{{monitor_agent_id}}",
        "groups":[
          {
                "groupid": "{{groupid}}"
          }
        ],

         "templates": [
            {
                "templateid": "{{templateid}}"
            }
        ],
        "interfaces": [
            {
                "type": 1,
                "main": 1,
                "useip": 1,
                "ip": "{{server_ip}}",
                "dns": "",
                "port": "10050"
            }
        ],

      {% if 'true' == 'monitor_proxy'|get_key %}
          "proxy_hostid":"{{proxyid}}"
      {% endif %}


    },
  require('../common/session.common')
}